

<%= link_to "新增項目", new_ab_path %>

<table>
	<tr>
		<th>消費時間	</th>
		<th>描述		</th>
		<th>主項目	</th>
		<th>副項目	</th>
		<th>其他項目 </th>
		<th>消費方式 </th>
		<th>其他項目</th>
	</tr>
	<% @abs.each do |ab| %>
	<tr>
		<td><%=ab.consume_date %>    </td>
		<td><%=ab.description %>     </td>
		<td><%=ab.main_class_id %>   </td>
		<td><%=ab.subclass_id %>     </td>
		<td><%=ab.third_class_id %>  </td>
		<td><%=ab.consume_way_id %>  </td>
		<td><%=ab.note %>      </td>
	</tr>
	<% end %>	
</table>






